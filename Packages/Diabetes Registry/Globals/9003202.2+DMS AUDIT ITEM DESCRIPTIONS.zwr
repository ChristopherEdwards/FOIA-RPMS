^BDMDMTX(8,11,35,11,8,0)="       TB - TX INCOMPLETE"
^BDMDMTX(8,11,35,11,9,0)="       TB - TX UNKNOWN"
^BDMDMTX(8,11,35,11,10,0)="       TB - TX UNTREATED"
