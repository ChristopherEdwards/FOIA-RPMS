^BGPINDE(27,11,121,0)=" "
^BGPINDE(27,11,122,0)="  - 1st Rx is Index Rx Date:  11/1/2004, # Days Prescribed=30"
^BGPINDE(27,11,123,0)="    Rx covers patient through 12/1/2004"
^BGPINDE(27,11,124,0)="  - 2nd Rx:  12/15/2004, # Days Prescribed=30"
^BGPINDE(27,11,125,0)="    Gap #1 = (12/15/2004-12/1/2004) = 14 days"
^BGPINDE(27,11,126,0)="    Rx covers patient through 1/14/2005"
^BGPINDE(27,11,127,0)="  - 3rd Rx:  2/01/2005, # Days Prescribed=30"
^BGPINDE(27,11,128,0)="    Gap #2 = (2/01/2005-1/14/2005) = 18, total # gap days = 32,"
