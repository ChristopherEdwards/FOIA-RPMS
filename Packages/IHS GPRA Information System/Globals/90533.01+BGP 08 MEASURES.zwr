^BGPINDE(27,11,112,0)="    Rx covers patient through 1/14/2005"
^BGPINDE(27,11,113,0)="  - 3rd RX:  1/10/2005, # Days Prescribed=30"
^BGPINDE(27,11,114,0)="    No gap days."
^BGPINDE(27,11,115,0)="    Rx covers patient through 2/13/2005"
^BGPINDE(27,11,116,0)="  - Index Rx Date 11/1/2004 + 114 days = 2/23/2005"
^BGPINDE(27,11,117,0)="  - Patient's 84th treatment day occurs on 2/7/2005, which is"
^BGPINDE(27,11,118,0)="    <= 2/23/2005 AND # gap days of 14 is less than 30."
^BGPINDE(27,11,119,0)=" "
