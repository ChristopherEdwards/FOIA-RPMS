^BGPINDM(102,11,132,0)="   Rx covers patient through 5/29/16"
^BGPINDM(102,11,133,0)="  -2nd Rx:  5/26/16, Days Supply=90"
^BGPINDM(102,11,134,0)="   Rx covers patient through 8/27/16"
^BGPINDM(102,11,135,0)="  -3rd Rx:  9/11/16, Days Supply=180"
^BGPINDM(102,11,136,0)="   Gap = (9/11/16 - 8/27/16) = 15 days"
^BGPINDM(102,11,137,0)="   Rx covers patient through 3/8/17"
^BGPINDM(102,11,138,0)=" "
^BGPINDM(102,11,139,0)="   Patient's measurement period:  3/1/16 through 12/31/16 = 306 Days"
^BGPINDM(102,11,140,0)="   Days patient was covered:  3/1/16 through 8/27/16 + 9/11/16 through "
^BGPINDM(102,11,141,0)="12/31/16 = 292 Days"
