^BGPINDM(102,11,153,0)="  Rx covers patient through 4/30/16"
^BGPINDM(102,11,154,0)="  -2nd Rx:  7/1/16, Days Supply=90"
^BGPINDM(102,11,155,0)="  Gap #1 = (7/1/16 - 4/30/16) = 61 days"
^BGPINDM(102,11,156,0)="  Rx covers patient through 9/28/16"
^BGPINDM(102,11,157,0)="  -3rd Rx:  10/1/16, Days Supply=90"
^BGPINDM(102,11,158,0)="  Gap #2 = (10/1/16 - 9/28/16) = 2 days"
^BGPINDM(102,11,159,0)="  Rx covers patient through 12/29/16"
^BGPINDM(102,11,160,0)="   "
