^DIC(71.2,0)="PROCEDURE MODIFIERS^71.2"
^DIC(71.2,0,"GL")="^RAMIS(71.2,"
^DIC(71.2,"%",0)="^1.005^1^1"
^DIC(71.2,"%",1,0)="RA"
