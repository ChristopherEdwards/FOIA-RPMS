^DIC(350,0)="INTEGRATED BILLING ACTION^350"
^DIC(350,0,"AUDIT")="@"
^DIC(350,0,"DD")="@"
^DIC(350,0,"DEL")="@"
^DIC(350,0,"GL")="^IB("
