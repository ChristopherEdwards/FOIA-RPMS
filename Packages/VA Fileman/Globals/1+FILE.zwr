^DIC(19.081,"%","B","XU",1)=""
^DIC(19.081,"%D",0)="^^6^6^2940913^^^^"
^DIC(19.081,"%D",1,0)="The Kernel Site Parameters File establishes when and how a log of option"
^DIC(19.081,"%D",2,0)="usage will be recorded in this file.  For the indicated time period, all"
^DIC(19.081,"%D",3,0)="specified options, namespaces, and users will be audited.  Each time an"
^DIC(19.081,"%D",4,0)="audit is put into effect, it is recommended that the number of audited"
^DIC(19.081,"%D",5,0)="entities be at a minimum so that disk space is not inadvertently wasted."
^DIC(19.081,"%D",6,0)="This file is cross-referenced by option."
^DIC(20,0)="NAME COMPONENTS^20"
^DIC(20,0,"AUDIT")=""
^DIC(20,0,"DD")="@"
^DIC(20,0,"DEL")="#"
