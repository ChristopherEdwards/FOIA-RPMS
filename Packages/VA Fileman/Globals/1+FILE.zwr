^DIC(8984.2,0,"RD")="#"
^DIC(8984.2,0,"WR")="@"
^DIC(8984.2,"%D",0)="^^4^4^2941104^^^"
^DIC(8984.2,"%D",1,0)="This is a word or phrase which will be used EXCLUSIVELY to find an"
^DIC(8984.2,"%D",2,0)="entry.  During a lookup, this file is checked first. If a shortcut matches"
^DIC(8984.2,"%D",3,0)="the user's entry, the corresponding entry will be displayed and no other"
^DIC(8984.2,"%D",4,0)="lookups will be performed."
^DIC(8984.3,0)="LOCAL SYNONYM^8984.3"
^DIC(8984.3,0,"AUDIT")="@"
^DIC(8984.3,0,"DD")="@"
^DIC(8984.3,0,"DEL")="@"
^DIC(8984.3,0,"GL")="^XT(8984.3,"
