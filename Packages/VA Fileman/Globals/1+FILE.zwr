^DIC(811.3,"%D",0)="^^7^7^3000321^^^^"
^DIC(811.3,"%D",1,0)="This file contains expanded taxonomies. It is used as a data cache so"
^DIC(811.3,"%D",2,0)="the expansion does not need to be continuously done. The expansion is"
^DIC(811.3,"%D",3,0)="rebuilt whenever a taxonomy is edited or one of the source files has"
^DIC(811.3,"%D",4,0)="changed since the expansion was last done. The source files are ICD0,"
^DIC(811.3,"%D",5,0)="ICD9, and ICPT."
^DIC(811.3,"%D",6,0)=" "
^DIC(811.3,"%D",7,0)="Entries in this file should never be set by hand."
^DIC(811.4,0)="REMINDER COMPUTED FINDINGS^811.4"
