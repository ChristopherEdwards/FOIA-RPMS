^DIC(9000010.11,"%D",0)="^^13^13^2950901^^"
^DIC(9000010.11,"%D",1,0)="This file has been designed for joint use by the Indian Health Service and"
^DIC(9000010.11,"%D",2,0)="the Department of Veteran Affairs."
^DIC(9000010.11,"%D",3,0)=" "
^DIC(9000010.11,"%D",4,0)="This file contains immunizations specific to a particular visit for a "
^DIC(9000010.11,"%D",5,0)="particular patient.  This file contains one record for each immunization."
^DIC(9000010.11,"%D",6,0)=" "
^DIC(9000010.11,"%D",7,0)="In the VA, if a CPT code is entered into PCE that represents an"
^DIC(9000010.11,"%D",8,0)="immunization, than an immunization will automatically be entered in the V"
^DIC(9000010.11,"%D",9,0)="Immunization file.  And vice versa, if an immunization is entered into PCE"
^DIC(9000010.11,"%D",10,0)="that has a related CPT code, then a V CPT entry will automatically be"
^DIC(9000010.11,"%D",11,0)="created with the CPT code for the immunization.  The PCE Code Mapping file"
^DIC(9000010.11,"%D",12,0)="contains the definitions of what immunization is related to what CPT code,"
^DIC(9000010.11,"%D",13,0)="and vice versa."
^DIC(9000010.12,0)="V SKIN TEST^9000010.12API"
^DIC(9000010.12,0,"AUDIT")="M"
^DIC(9000010.12,0,"DD")="@"
^DIC(9000010.12,0,"DEL")="M"
^DIC(9000010.12,0,"GL")="^AUPNVSK("
^DIC(9000010.12,0,"LAYGO")="M"
