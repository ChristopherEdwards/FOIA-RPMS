^DIC(351,"%D",1,0)="DO NOT delete entries in this file.  DO NOT edit data in this file"
^DIC(351,"%D",2,0)="with VA File Manager."
^DIC(351,"%D",3,0)=" "
^DIC(351,"%D",4,0)="This file was introduced with v1.5 of Integrated Billing in"
^DIC(351,"%D",5,0)="conjunction with the automation of bills for Means Test/Category C"
^DIC(351,"%D",6,0)="charges.  The file is used to create and maintain billing clocks in"
^DIC(351,"%D",7,0)="which Category C patients may be charged co-payment and per diem"
^DIC(351,"%D",8,0)="charges for Hospital or Nursing Home Care, as well as outpatient"
^DIC(351,"%D",9,0)="visits.  This file was initially populated by the Means Test data"
