^DIC(9.95,0,"WR")="@"
^DIC(9.95,"%D",0)="^^19^19^3050721^"
^DIC(9.95,"%D",1,0)="This is the parameter file that controls certain functions of the Patch "
^DIC(9.95,"%D",2,0)="Monitor software:"
^DIC(9.95,"%D",3,0)=" "
^DIC(9.95,"%D",4,0)="   .01          SITE NAME"
^DIC(9.95,"%D",5,0)="   1            MAIL GROUPS  (multiple)"
^DIC(9.95,"%D",6,0)="   2            NUMBER OF DAYS TO KEEP DATA"
^DIC(9.95,"%D",7,0)="   3            DELETE INSTALLED PATCHES?"
^DIC(9.95,"%D",8,0)=" "
^DIC(9.95,"%D",9,0)="Site name: Free text name of the facility."
^DIC(9.95,"%D",10,0)=" "
^DIC(9.95,"%D",11,0)="Mail groups: groups that wish to receive the uninstalled bulletin."
^DIC(9.95,"%D",12,0)=" "
^DIC(9.95,"%D",13,0)="Number of days to keep data: number of days to allow data to remain, "
^DIC(9.95,"%D",14,0)="provided field 3 is answered NO.  If field 3 is answered YES, this field "
^DIC(9.95,"%D",15,0)="is ignored."
^DIC(9.95,"%D",16,0)=" "
^DIC(9.95,"%D",17,0)="Delete installed patches? - YES allows the nightly job to remove patches "
^DIC(9.95,"%D",18,0)="shown as installed.  NO does not remove them.  If this field is answered "
^DIC(9.95,"%D",19,0)="YES, field 2 is ignored."
^DIC(14.4,0)="TASKS^14.4"
