^DIC(176.001,"%D",6,0)=" "
^DIC(176.001,"%D",7,0)="        Col.    Description"
^DIC(176.001,"%D",8,0)="       *RXCUI   RxNorm Unique identifier for concept (concept ID)"
^DIC(176.001,"%D",9,0)="        LAT     Language of Term"
