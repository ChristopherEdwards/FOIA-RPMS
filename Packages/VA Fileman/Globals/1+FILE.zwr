^DIC(9000014,0,"RD")="M"
^DIC(9000014,0,"WR")="M"
^DIC(9000014,"%D",0)="^1.001^7^7^3081114^^^^"
^DIC(9000014,"%D",1,0)="This file contains a history of family health problems for a given"
^DIC(9000014,"%D",2,0)="patient. These are not the patient's problems, but those of some"
^DIC(9000014,"%D",3,0)="immediate member of the family.  The information is gathered as a"
^DIC(9000014,"%D",4,0)="result of a patient visit at which the patient provides the history."
^DIC(9000014,"%D",5,0)="The file contains backward pointers to the IHS Patient file and"
^DIC(9000014,"%D",6,0)="visit file, and data must exist in both of these files for the"
^DIC(9000014,"%D",7,0)="patient and visit before data can be entered here."
