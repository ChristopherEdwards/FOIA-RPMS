^APCLDMTX(6,11,35,11,8,0)="       TB - TX INCOMPLETE"
^APCLDMTX(6,11,35,11,9,0)="       TB - TX UNKNOWN"
^APCLDMTX(6,11,35,11,10,0)="       TB - TX UNTREATED"
^APCLDMTX(6,11,35,11,11,0)="  Cumulative Audit: "
^APCLDMTX(6,11,35,11,12,0)="N/A  (SEE TB STATUS BELOW)"
^APCLDMTX(6,11,35,11,13,0)="  EPI Info Export: "
^APCLDMTX(6,11,35,11,14,0)="The last TB health factor is examined and recoded as follows:"
^APCLDMTX(6,11,35,11,15,0)="1 - Yes if factor is TB - TX COMPLETE"
^APCLDMTX(6,11,35,11,16,0)="2 - No if factor is TB - TX INCOMPLETE or TB - TX UNTREATED"
^APCLDMTX(6,11,35,11,17,0)="4 - Unknown if factor is TB - TX UNKNOWN"
^APCLDMTX(6,11,36,0)="IF PPD NEG, LAST PPD DATE"
