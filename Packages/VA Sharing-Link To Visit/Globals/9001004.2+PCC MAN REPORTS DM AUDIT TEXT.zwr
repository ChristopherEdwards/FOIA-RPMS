^APCLDMTX(4,11,35,11,6,0)="factors are:"
^APCLDMTX(4,11,35,11,7,0)="       TB - TX COMPLETE"
^APCLDMTX(4,11,35,11,8,0)="       TB - TX INCOMPLETE"
^APCLDMTX(4,11,35,11,9,0)="       TB - TX UNKNOWN"
^APCLDMTX(4,11,35,11,10,0)="       TB - TX UNTREATED"
^APCLDMTX(4,11,35,11,11,0)="  Cumulative Audit: "
^APCLDMTX(4,11,35,11,12,0)="N/A  (SEE TB STATUS BELOW)"
^APCLDMTX(4,11,35,11,13,0)="  EPI Info Export: "
^APCLDMTX(4,11,35,11,14,0)="The last TB health factor is examined and recoded as follows:"
^APCLDMTX(4,11,35,11,15,0)="1 - Yes if factor is TB - TX COMPLETE"
^APCLDMTX(4,11,35,11,16,0)="2 - No if factor is TB - TX INCOMPLETE or TB - TX UNTREATED"
^APCLDMTX(4,11,35,11,17,0)="4 - Unknown if factor is TB - TX UNKNOWN"
^APCLDMTX(4,11,36,0)="IF PPD NEG, LAST PPD DATE"
^APCLDMTX(4,11,36,11,0)="^9001004.21111^9^9^3050622^^^^"
^APCLDMTX(4,11,36,11,1,0)="If the value of the PPD Status is NEGATIVE then the date of the last PPD"
